<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WorldsFactory.works.storyInterpreterWork.export.ui.ExportStoryInterpretersDataView">
  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
      <TextBlock Text="Export" FontWeight="Bold"/>

      <Button Name="SelectFolderButton" Content="Select target folder" />

      <TextBox Name="PathInput" Watermark="Path"/>

      <Button Name="ExportButton" Content="Export"/>

      <TextBlock Name="ExportStatus" Text="" />
  </StackPanel>
</UserControl>
